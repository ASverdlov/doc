#: ../download_16.rst:152
msgid "Fedora"
msgstr ""

#: ../download_16.rst:154
msgid ""
"We maintain an always up-to-date Fedora package repository. At the "
"moment, the repository contains builds for Fedora 23 and 24."
msgstr ""

#: ../download_16.rst
msgid "In these instructions:"
msgstr ""

#: ../download_16.rst
msgid ""
"``$releasever`` (i.e. Fedora release version) must be 23 or 24 or "
"rawhide, and"
msgstr ""

#: ../download_16.rst
msgid "``$basearch`` (i.e. base architecture) must be either i386 or x86_64."
msgstr ""

#: ../download_16.rst:165
msgid ""
"sudo rm -f /etc/yum.repos.d/*tarantool*.repo\n"
"sudo tee /etc/yum.repos.d/tarantool_1_6.repo <<- EOF\n"
"[tarantool_1_6]\n"
"name=Fedora-\\$releasever - Tarantool\n"
"baseurl=http://download.tarantool.org/tarantool/1.6/fedora/\\$releasever/\\$basearch/"
"\n"
"gpgkey=http://download.tarantool.org/tarantool/1.6/gpgkey\n"
"repo_gpgcheck=1\n"
"gpgcheck=0\n"
"enabled=1\n"
"\n"
"[tarantool_1_6-source]\n"
"name=Fedora-\\$releasever - Tarantool Sources\n"
"baseurl=http://download.tarantool.org/tarantool/1.6/fedora/\\$releasever/SRPMS"
"\n"
"gpgkey=http://download.tarantool.org/tarantool/1.6/gpgkey\n"
"repo_gpgcheck=1\n"
"gpgcheck=0\n"
"EOF\n"
"\n"
"sudo dnf -q makecache -y --disablerepo='*' --enablerepo='tarantool_1_6'\n"
"sudo dnf -y install tarantool"
msgstr ""

#: ../download_16.rst:188
msgid "RHEL 6 and CentOS 6"
msgstr ""

#: ../download_16.rst:190
msgid ""
"We maintain an always up-to-date package repository for RHEL 6 "
"derivatives. You may need to enable the `EPEL`_ repository for some "
"packages."
msgstr ""

#: ../download_16.rst
msgid "``$releasever`` (i.e. CentOS release version) must be 6, and"
msgstr ""

#: ../download_16.rst:201
msgid ""
"# Enable EPEL repository\n"
"sudo yum -y install http://dl.fedoraproject.org/pub/epel/epel-release-"
"latest-6.noarch.rpm\n"
"sed 's/enabled=.*/enabled=1/g' -i /etc/yum.repos.d/epel.repo\n"
"\n"
"# Add Tarantool repository\n"
"sudo rm -f /etc/yum.repos.d/*tarantool*.repo\n"
"sudo tee /etc/yum.repos.d/tarantool_1_6.repo <<- EOF\n"
"[tarantool_1_6]\n"
"name=EnterpriseLinux-\\$releasever - Tarantool\n"
"baseurl=http://download.tarantool.org/tarantool/1.6/el/6/\\$basearch/\n"
"gpgkey=http://download.tarantool.org/tarantool/1.6/gpgkey\n"
"repo_gpgcheck=1\n"
"gpgcheck=0\n"
"enabled=1\n"
"\n"
"[tarantool_1_6-source]\n"
"name=EnterpriseLinux-\\$releasever - Tarantool Sources\n"
"baseurl=http://download.tarantool.org/tarantool/1.6/el/6/SRPMS\n"
"gpgkey=http://download.tarantool.org/tarantool/1.6/gpgkey\n"
"repo_gpgcheck=1\n"
"gpgcheck=0\n"
"EOF\n"
"\n"
"# Update metadata\n"
"sudo yum makecache -y --disablerepo='*' --enablerepo='tarantool_1_6' "
"--enablerepo='epel'\n"
"\n"
"# Install tarantool\n"
"sudo yum -y install tarantool"
msgstr ""

