"gpgkey=http://download.tarantool.org/tarantool/1.8/gpgkey\n"
"repo_gpgcheck=1\n"
"gpgcheck=0\n"
"EOF\n"
"\n"
"sudo dnf -q makecache -y --disablerepo='*' --enablerepo='tarantool_1_8'\n"
